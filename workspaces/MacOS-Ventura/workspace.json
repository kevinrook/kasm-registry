{
    "name": "macOS Ventura Workspace",
    "description": "A Kasm Workspace for macOS-Ventura using Docker",
    "max_users": 1,
    "compatibility": "1.14.x",
    "workspace_app": {
      "name": "Docker",
      "version": "latest",
      "settings": {
        "image": "sickcodes/docker-osx:ventura",
        "ports": [
          {
            "internal": 10022,
            "external": 50922
          }
        ],
        "devices": [
          "/dev/kvm"
        ],
        "volumes": [
          {
            "host_path": "/tmp/.X11-unix",
            "container_path": "/tmp/.X11-unix"
          }
        ],
        "environment": {
          "DISPLAY": "${DISPLAY:-:0.0}",
          "GENERATE_UNIQUE": "true",
          "MASTER_PLIST_URL": "https://raw.githubusercontent.com/sickcodes/osx-serial-generator/master/config-custom.plist"
        }
      }
    }
  }
  